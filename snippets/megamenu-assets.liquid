<script>

$(document).ready(function() {

    // wrap baby menu
    // $('.enable-new-megamenu .babymenu-holder').each(function() {
    //   $(this).find('.child-item-wrap:nth-child(2), .child-item-wrap:nth-child(3)').wrapAll('<div />');
    //   $(this).find('.child-item-wrap:nth-child(3), .child-item-wrap:nth-child(4)').wrapAll('<div />');
    // })

    // Add default class for megamenu item
    $('.enable-new-megamenu .main-main-item').hover(function() {
      $('.enable-new-megamenu .submenu-holder .babymenu-item').removeClass('babymenu-active');
      $('.enable-new-megamenu .submenu-holder-right .babymenu').removeClass('babymenu-active');

      $(this).find('.submenu-holder .babymenu-item').first().addClass("babymenu-active");
      $(this).find('.submenu-holder-right .babymenu').first().addClass("babymenu-active");
    });

    $('.enable-new-megamenu .submenu-holder .babymenu-item').hover(function() {
      $('.enable-new-megamenu .submenu-holder .babymenu-item').removeClass('babymenu-active');
      $('.enable-new-megamenu .submenu-holder-right .babymenu').removeClass('babymenu-active');

      var menuItem = $(this).data('babymenu');
      $(this).addClass('babymenu-active');
      $(this).parents('.submenu-container').find('.submenu-holder-right [data-babymenu="'+menuItem+'"]').addClass("babymenu-active");
    });
  

    // mobile megamenu 
    $('a.main-item.mobile-menu-link').each(function() {
      $(this).click(function(e) {
        e.preventDefault();
        $(this).parent().toggleClass("mobile-arrow-down")
        $(this).parent().find(".mobile-submenu").toggleClass("mobile-submenu-open")
      })
    });

    $('a.mobile-babymenu-link').each(function() {
      $(this).click(function(e) {
        e.preventDefault();
        $(this).parent().toggleClass("mobile-babymenu-open");
        $(".mobile-new-megamenu-enable .sidebar__body nav").toggleClass("mobile-babymenu-parent-open");
        if($(".mobile-new-megamenu-enable .sidebar__body nav").hasClass("mobile-babymenu-parent-open")){
          var babymenuHeight = $('.mobile-new-megamenu-enable li.mobile-babymenu-item.mobile-babymenu-open').height();
          $('.mobile-new-megamenu-enable .sidebar__body nav').css('height' , babymenuHeight);
        } else {
          $('.mobile-new-megamenu-enable .sidebar__body nav').css('height' , 'auto');
        }
        
      })
    });

    $('.new-megamenu-mobile-button').click(function(){
      $('body').toggleClass('new-scroll-hide');
      $(this).toggleClass('mobile-menu-open-button');
      $('sidebar-drawer#site-menu-sidebar').toggleClass('mobile-menu-open');
    });
    var sidemenuHeight = $('announcement-bar').height() + $('.header-container.header-container--top.hide-border-on-portable').height();
    $('sidebar-drawer.mobile-new-megamenu-enable').css('top' , sidemenuHeight);

});

</script> 

















